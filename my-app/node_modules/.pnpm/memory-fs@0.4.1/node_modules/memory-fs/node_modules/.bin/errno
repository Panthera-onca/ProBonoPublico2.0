#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/errno@0.1.8/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/memory-fs@0.4.1/node_modules/memory-fs/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/memory-fs@0.4.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../errno@0.1.8/node_modules/errno/cli.js" "$@"
else
  exec node  "$basedir/../../../../../errno@0.1.8/node_modules/errno/cli.js" "$@"
fi
