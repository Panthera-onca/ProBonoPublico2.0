#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/uuid@3.4.0/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/sockjs@0.3.20/node_modules/sockjs/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/sockjs@0.3.20/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
else
  exec node  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
fi
