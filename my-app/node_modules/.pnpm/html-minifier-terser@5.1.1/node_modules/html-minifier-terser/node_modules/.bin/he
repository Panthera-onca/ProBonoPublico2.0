#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/he@1.2.0/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
