#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/json5@1.0.1/node_modules/json5/lib/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/json5@1.0.1/node_modules/json5/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/json5@1.0.1/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/loader-utils@1.4.0/node_modules/loader-utils/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/loader-utils@1.4.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../json5@1.0.1/node_modules/json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../json5@1.0.1/node_modules/json5/lib/cli.js" "$@"
fi
