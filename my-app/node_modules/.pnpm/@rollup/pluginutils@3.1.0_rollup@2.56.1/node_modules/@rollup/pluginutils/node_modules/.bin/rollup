#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rollup@2.56.1/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rollup@2.56.1/node_modules/rollup/dist/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rollup@2.56.1/node_modules/rollup/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rollup@2.56.1/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/@rollup/pluginutils@3.1.0_rollup@2.56.1/node_modules/@rollup/pluginutils/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/@rollup/pluginutils@3.1.0_rollup@2.56.1/node_modules/@rollup/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/@rollup/pluginutils@3.1.0_rollup@2.56.1/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/@rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../rollup@2.56.1/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../../../rollup@2.56.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
