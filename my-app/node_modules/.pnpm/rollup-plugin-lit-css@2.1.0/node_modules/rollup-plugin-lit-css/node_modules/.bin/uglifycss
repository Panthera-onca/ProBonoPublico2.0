#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/uglifycss@0.0.29/node_modules/uglifycss/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/uglifycss@0.0.29/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rollup-plugin-lit-css@2.1.0/node_modules/rollup-plugin-lit-css/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rollup-plugin-lit-css@2.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uglifycss@0.0.29/node_modules/uglifycss/uglifycss" "$@"
else
  exec node  "$basedir/../../../../../uglifycss@0.0.29/node_modules/uglifycss/uglifycss" "$@"
fi
