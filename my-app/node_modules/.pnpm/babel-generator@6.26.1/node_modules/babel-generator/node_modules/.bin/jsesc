#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/bin/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/jsesc@1.3.0/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/babel-generator@6.26.1/node_modules/babel-generator/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/babel-generator@6.26.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsesc@1.3.0/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../jsesc@1.3.0/node_modules/jsesc/bin/jsesc" "$@"
fi
