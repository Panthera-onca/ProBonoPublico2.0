#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/move-concurrently@1.0.1/node_modules/move-concurrently/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/move-concurrently@1.0.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
