#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/rimraf@3.0.2/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules:/mnt/c/Users/nikol/Documents/node_modules:/mnt/c/Users/nikol/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/cacache@15.2.0/node_modules/cacache/node_modules:/mnt/c/Users/nikol/Documents/my-app/node_modules/.pnpm/cacache@15.2.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
